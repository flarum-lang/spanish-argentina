{
	"name": "flarum-lang/spanish-argentina",
	"type": "flarum-extension",
	"description": "Spanish (Argentina) language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/spanish-argentina",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"es_AR-locale",
		"spanish-argentina-locale",
		"spanish-argentina"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/spanish-argentina/issues",
		"source": "https://github.com/flarum-lang/spanish-argentina"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Spanish (Argentina)",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "es_AR",
			"title": "Spanish (Argentina)"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
